{
  "name": "nxrouter",
  "description": "Simple, functional http-client router for JavaScript/TypeScript",
  "version": "2.0.0",
  "type": "commonjs",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "node ./test/index.mjs",
    "format": "prettier --write \"./{src,test}/**/*.{js,ts,mjs,mts}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neplextech/nxrouter.git"
  },
  "keywords": [
    "router",
    "http",
    "client",
    "api",
    "node",
    "javascript",
    "typescript",
    "fetch",
    "functional"
  ],
  "author": "Twilight",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/neplextech/nxrouter/issues"
  },
  "homepage": "https://github.com/neplextech/nxrouter#readme",
  "devDependencies": {
    "@types/node": "^22.14.0",
    "prettier": "^3.5.3",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "use-macro": "^1.1.0"
  },
  "packageManager": "yarn@4.9.0"
}