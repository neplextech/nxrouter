{
  "name": "nxrouter",
  "packageManager": "yarn@3.5.0",
  "description": "Simple, functional http-client router for JavaScript/TypeScript",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
