    "javascript",
    "typescript",
    "fetch",
    "functional"
  ],
  "author": "Neplex Technologies",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/neplextech/nxrouter/issues"
  },
  "homepage": "https://github.com/neplextech/nxrouter#readme",
  "devDependencies": {
    "prettier": "^2.8.8",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
  }
